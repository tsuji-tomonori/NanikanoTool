{
  "app": "python app.py",
  "requireApproval": "never",
  "fail": true,
  "versionReporting": false,
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__init__.py",
      "python/__pycache__",
      "tests"
    ]
  },
  "context": {
    "PROJECT_NAME": "NanikanoTool",
    "lambda_env": {
      "LOG_LEVEL": "INFO",
      "PKEY": "archive_url",
      "N_RETRY": "5"
    }
  }
}